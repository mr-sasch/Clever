mixin jal()
	+b.jal.container.py-5&attributes(attributes)
		#jaluziAnchor
		block
		mixin titleLeft(title)
			.title-left
				.title-left__title= title
				.title-left__sep
		+titleLeft('Жалюзи')
		.row
			.col-12.col-lg-3.mb-5.mb-lg-0
				mixin sideNav(i1, i2, i3, i4, i5, i6)
					div.side-nav(class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical")
						a.side-nav__li(class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#i1" role="tab" aria-controls="v-pills-home" aria-selected="true")= i1
						a.side-nav__li(class="nav-link" id="v-pills-profile-tab" data-toggle="pill" href="#i2" role="tab" aria-controls="v-pills-profile" aria-selected="false")= i2
						a.side-nav__li(class="nav-link" id="v-pills-messages-tab" data-toggle="pill" href="#i3" role="tab" aria-controls="v-pills-messages" aria-selected="false")= i3
						a.side-nav__li(class="nav-link" id="v-pills-settings-tab" data-toggle="pill" href="#i4" role="tab" aria-controls="v-pills-settings" aria-selected="false")= i4
						a.side-nav__li(class="nav-link" id="v-pills-settings-tab" data-toggle="pill" href="#i5" role="tab" aria-controls="v-pills-settings" aria-selected="false")= i5
						a.side-nav__li(class="nav-link" id="v-pills-settings-tab" data-toggle="pill" href="#i6" role="tab" aria-controls="v-pills-settings" aria-selected="false")= i6
				+sideNav('Горизонтальные жалюзи', 'Вертикальные жалюзи', 'Рулонные шторы', 'Фотожалюзи', 'Шторы-плиссе', 'Жалюзи с электроприводом')
			.col.jal__right-wrap.ml-0.ml-lg-3
				.jal__right
					// Миксин заголовка
					mixin titleSimple(title)
						.title-simple= title
					// Миксин карточки
					mixin jalCard(ttl, price, btn)
						.col-12.col-sm-6.col-lg-4.mb-5
							.jal__card.d-flex.flex-column.justify-content-between
								.jal__card-img(style='background-image: url("images/jalcard-img1.png")')
								//- img(src= img).jal__card-img
								.jal__card-title= ttl
								.jal__card-price от 
									.jal__card-price-num= price
								.jal__card-more 
									a(href='').jal__card-more-a Подробнее
					div(class="tab-content" id="#i1")
						div(class="tab-pane fade show active" id="i1" role="tabpanel" aria-labelledby="pills-home-tab")
							.row
								+jalCard('Пластиковые горизонтальные жалюзи', '7000 RUB', 'btn')
								+jalCard('Алюминиевые горизонтальные жалюзи', '7000 RUB', 'btn')
								+jalCard('Деревянные горизонтальные жалюзи', '7000 RUB', 'btn')
								+jalCard('Кассетные горизонтальные жалюзи', '7000 RUB', 'btn')
								+jalCard('Жалюзи из бамбука', '7000 RUB', 'btn')
								+jalCard('Жалюзи из бамбука', '7000 RUB', 'btn')
							+titleSimple('Горизонтальные жалюзи')
							.jal__right-txt
								.jal__right-txt-preview Vivamus magna justo, lacinia eget consectetur sed, convallis at tellus. Praesent sapien massa, convallis a pellentesque nec, egestas non nisi. Donec rutrum congue leo eget malesuada. Pellentesque in ipsum id orci porta dapibus.
								.jal__right-txt-more#collapseJalOne.collapse
									.jal__right-txt-more-wrap Vestibulum ante ipsum primis in faucibus orci luctus et ultrices posuere cubilia Curae; Donec velit neque, auctor sit amet aliquam vel, ullamcorper sit amet ligula. Proin eget tortor risus. Vivamus suscipit tortor eget felis porttitor volutpat. Donec sollicitudin molestie malesuada. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Quisque velit nisi, pretium ut lacinia in, elementum id enim.
								.jal__right-txt-more-btn.collapsed(data-toggle="collapse" data-target="#collapseJalOne") Смотреть весь текст
